{
    "name": "tes/cms-builder",
    "description": "Allows easy set up TES Drupal sites",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alexpott/platform-docker"
        },
        {
            "type": "vcs",
            "url": "https://github.com/alexpott/docker-helper"
        },
        {
            "type": "vcs",
            "url": "https://github.com/alexpott/toolstack-helper"
        }
    ],
    "require": {
        "mglaman/toolstack-helper": "dev-fix-drupal-detection",
        "mglaman/platform-docker": "dev-tes",
        "mglaman/docker-helper": "dev-exec",
        "symfony/console": "^2.7",
        "symfony/filesystem": "^3.0",
        "symfony/process": "^3.0",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "tes\\CmsBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tes\\CmsBuilder\\Tests\\": "tests"
        }
    }
}
